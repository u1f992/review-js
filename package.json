{
  "name": "@u1f992/review-js",
  "version": "0.1.1",
  "description": "Re:VIEW markup language parser and converter compiled to JavaScript via Opal",
  "type": "module",
  "main": "dist/review.js",
  "exports": {
    ".": {
      "import": "./dist/review.js",
      "types": "./dist/review.d.ts"
    }
  },
  "types": "dist/review.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bundle install && bundle exec rake build",
    "build:browser": "node scripts/build-browser.js",
    "clean": "bundle exec rake clean",
    "test": "node --test test/*.test.js",
    "serve": "http-server -o /test/browser.html"
  },
  "keywords": [
    "review",
    "markup",
    "parser",
    "converter",
    "html",
    "markdown",
    "latex",
    "documentation",
    "technical-writing",
    "opal",
    "ruby"
  ],
  "author": "Koutaro Mukai",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/u1f992/review-js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@u1f992/mecab-wasm": "^1.0.3",
    "csv-parse": "^6.1.0",
    "hast-util-to-html": "^9.0.5",
    "meaw": "^10.0.0",
    "memfs": "^4.51.1",
    "refractor": "^5.0.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/node": "^20.0.0",
    "esbuild": "^0.27.2",
    "http-server": "^14.1.1"
  }
}
